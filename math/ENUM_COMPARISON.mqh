// Value Comparison Methods

#ifndef CUSTOM_ENUM_COMPARISON

enum ENUM_COMPARISON {
    EQUAL = 1,                // ==
    IDENTICAL,                // ===
    NOT_EQUAL,                // !=
    NOT_IDENTICAL,            // !==
    LESS_THAN,                // <
    GREATER_THAN,             // >
    LESS_EQUAL_THAN,          // <=
    GREATER_EQUAL_THAN        // >=
};

#endif
